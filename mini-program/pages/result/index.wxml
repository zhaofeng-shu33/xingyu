<!--结果展示页面-->
<view class="container">
      <view class="weui-cell weui-cell_input">
        <view class="weui-cell__hd">
            <view class="weui-label">第</view>
        </view>
      <view class="weui-cell__bd">
            <input class="weui-input" placeholder="深大校历周数" value="{{week}}" bindinput="setweek"/>          
      </view>
      <view class="weui-cell__ft"> <view class="weui-label">周</view></view>
      </view>

  <view>
            <picker bindchange="bindPickerChange" value="{{groupIndex}}" range="{{group_list}}">
                <button type="default">{{group_name}}</button>                
            </picker>
  </view>
    <view class="weui-btn-area weui-flex">
        <button class="weui-btn" type="primary" bindtap="search">常规活动查询</button>
        <navigator url="/pages/result/special_activity">
          <button class="weui-btn" type="secondary">特色活动查询</button>
        </navigator>
    </view>  


   <!--列表展示全部志愿者 -->
   全部志愿者
  <view class="weui-cells weui-cells_after-title">
    <view wx:for="{{group_data}}" wx:key="value">
    <label  class="weui-cell weui-check__label">
        <view class="weui-cell__bd"> {{item.name}}</view>
        <view class="weui-cell__ft">{{item.school}}</view>
    </label>
    </view>
  </view>

</view>
