      <view class="weui-cells weui-cells_after-title">
        <navigator url="/pages/result/activity_detail?activity_name={{item.name}}&activity_location={{item.location}}&activity_time={{item.time}}" wx:for="{{special_activity_list}}" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
            <view class="weui-cell__bd"> {{item.location}}{{item.name}}</view>
